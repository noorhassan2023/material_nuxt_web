    "bootstrap": "^4.6.2",
    "bootstrap-vue": "^2.22.0",
    "cookie-universal-nuxt": "^2.2.2",
    "core-js": "^3.25.3",
    "jquery": "^3.6.4",
    "nuxt": "^2.15.8",
    "nuxt-gsap-module": "^2.0.0",
